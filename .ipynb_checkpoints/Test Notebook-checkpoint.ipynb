    "def ss(number_list, n):\n",
    "    found = False\n",
    "    for i in number_list:\n",
    "        if i == n:\n",
    "            found = True\n",
    "            break\n",
    "    return found\n",
    "\n",
    "\n",
    "numbers = range(0, 100)\n",
    "s1 = ss(numbers, 2)\n",
    "print(s1)\n",
    "s2 = ss(numbers, 202)\n",
    "print(s2)\n",
    "\n"
   ]
